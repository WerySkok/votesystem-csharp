{
  "name": "votesystem-csharp",
  "scripts": {
    "build": "npm run build-css && npm run compress",
    "build-css": "postcss tailwind.css -o ./wwwroot/css/main.css",
    "compress": "minify ./wwwroot/css/main.css > ./wwwroot/css/main.min.css"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "minify": "^10.3.0",
    "postcss": "^8.4.26",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.0",
    "postcss-preset-env": "^9.0.0",
    "preline": "^1.9.0",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3"
  }
}
